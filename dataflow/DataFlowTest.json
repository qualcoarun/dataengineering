				"          LOV_LOW as integer,",
				"          LOV_HIGH as integer,",
				"          LOV_INTERNAL_DESC as string,",
				"          LOV_COMMENTS as string,",
				"          ENT_CODE as integer,",
				"          LOV_IS_SUBMITTED as boolean,",
				"          LOV_IS_APPROVED as boolean,",
