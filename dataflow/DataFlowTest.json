			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          LOV_CODE as integer,",
				"          LOV_CREATED as timestamp,",
				"          LOV_LAST_UPD as timestamp,",
				"          LOV_CREATED_BY as string,",
